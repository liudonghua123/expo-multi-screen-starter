{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "prettier",
    "eslint-plugin-import-helpers"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "no-use-before-define": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allowAfterThis": true
      }
    ],
    "no-unused-vars": 0,
    "react/no-unused-prop-types": 0,
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "no-shadow": 0,
    "react/prop-types": 0
  },
  "globals": {
    "__DEV__": 0,
    "describe": 0,
    "expect": 0,
    "fetch": 0,
    "it": 0,
    "jest": 0
  }
}